net: "deepTutorial/config/deffontnet-10/train_val_10.prototxt"

test_iter: 4
test_interval: 50

base_lr: 0.001

lr_policy: "step"
gamma: 0.1
stepsize: ${STEP_SIZE}
max_iter: ${MAX_ITERATIONS}

momentum: 0.9
weight_decay: 0.0005

display: 50

snapshot: ${SNAPSHOT_INTERVAL}
snapshot_prefix: "deepTutorial/model/deffontnet-10/train_train_10"

solver_mode: GPU
